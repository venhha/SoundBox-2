<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="5dp" />
            <gradient android:angle="-180" android:startColor="@color/material_dynamic_primary0" android:endColor="@color/material_dynamic_neutral0" />
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="5dp" />
            <gradient android:angle="180" android:startColor="@color/material_dynamic_neutral0" android:endColor="@color/material_dynamic_neutral0" />
        </shape>
    </item>
</selector>